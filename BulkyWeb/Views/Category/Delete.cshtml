@model Category

<form method="post" class="form">

	<h2>Category Name</h2>
	<input asp-for="Name" disabled class="form-control" />

	<h2 class="mt-3">Display Order</h2>
	<input asp-for="DisplayOrder" disabled class="form-control" />

	<input type="submit" class="mt-3 btn btn-primary" value="Delete" />
	<a class="btn btn-outline-secondary mt-3" asp-controller="Category" asp-action="Index">Back to list</a>

</form>